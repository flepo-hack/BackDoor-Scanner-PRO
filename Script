-- Final Enhanced Version: Backdoor Scanner - PRO for Fluxus Executor
-- Features: Advanced Backdoor scanning, Modern UI, Anti-Cheat, Anti-Detect, Draggable UI, Error Handling, and Extended Logging

local scanner = {}

-- Anti-Cheat & Anti-Detect Bypass
local function antiCheatBypass()
    -- Prevent detection and kicks (e.g., error 267 bypass)
    local mt = getrawmetatable(game)
    setreadonly(mt, false)
    local oldIndex = mt.__index
    mt.__index = function(t, k)
        if k == "Kick" or k == "Destroy" then return function() end end
        return oldIndex(t, k)
    end
end

-- Extended Backdoor Scan Function (Three Passes with In-Depth Detection)
local function scanForBackdoors()
    local backdoors = {}
    local methods = {"RemoteEvent", "RemoteFunction", "LoadString", "require", "ModuleScript", "Script"}
    
    for i = 1, 3 do
        for _, obj in pairs(game:GetDescendants()) do
            for _, method in pairs(methods) do
                -- Check if object is a method that can be used as a backdoor
                if obj:IsA(method) or (obj:IsA("ModuleScript") and obj.Source:find("require")) then
                    table.insert(backdoors, obj:GetFullName())
                end
                -- Check if object has suspicious behaviors or strings
                if obj:IsA("Script") and obj.Source:find("require") then
                    table.insert(backdoors, "Suspicious Script Found: " .. obj:GetFullName())
                end
            end
        end
    end
    
    return backdoors
end

-- UI Creation
local function createUI()
    local TweenService = game:GetService("TweenService")
    
    local ScreenGui = Instance.new("ScreenGui")
    local StartFrame = Instance.new("Frame")
    local MainFrame = Instance.new("Frame")
    local ProgressBar = Instance.new("Frame")
    local Progress = Instance.new("Frame")
    local ScanButton = Instance.new("TextButton")
    local MinimizeButton = Instance.new("TextButton")
    local CloseButton = Instance.new("TextButton")
    local ExecuteButton = Instance.new("TextButton")
    local ScriptBox = Instance.new("TextBox")
    local StatusLabel = Instance.new("TextLabel")
    local TitleLabel = Instance.new("TextLabel")
    local WarningLabel = Instance.new("TextLabel")
    
    ScreenGui.Parent = game.CoreGui
    
    -- Setup Start UI Elements
    StartFrame.Size = UDim2.new(0, 400, 0, 250)
    StartFrame.Position = UDim2.new(0.5, -200, 0.5, -125)
    StartFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    StartFrame.BorderSizePixel = 2
    StartFrame.Draggable = true
    StartFrame.Active = true
    StartFrame.BorderMode = Enum.BorderMode.Outline
    StartFrame.BorderColor3 = Color3.fromRGB(70, 70, 70)
    StartFrame.BackgroundTransparency = 0.1
    
    -- Title Label
    TitleLabel.Size = UDim2.new(0, 380, 0, 40)
    TitleLabel.Position = UDim2.new(0.5, -190, 0.1, 0)
    TitleLabel.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    TitleLabel.Text = "Backdoor Scanner PRO"
    TitleLabel.TextSize = 24
    TitleLabel.Parent = StartFrame

    -- Warning Label
    WarningLabel.Size = UDim2.new(0, 380, 0, 30)
    WarningLabel.Position = UDim2.new(0.5, -190, 0.3, 0)
    WarningLabel.BackgroundColor3 = Color3.fromRGB(255, 165, 0)
    WarningLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
    WarningLabel.Text = "Warning: Freezing may occur during scan"
    WarningLabel.TextSize = 18
    WarningLabel.Parent = StartFrame
    
    -- Progress Bar
    ProgressBar.Size = UDim2.new(0, 380, 0, 20)
    ProgressBar.Position = UDim2.new(0.5, -190, 0.5, 40)
    ProgressBar.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    ProgressBar.Parent = StartFrame
    
    Progress.Size = UDim2.new(0, 0, 1, 0)
    Progress.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    Progress.Parent = ProgressBar
    
    -- Status Label
    StatusLabel.Size = UDim2.new(0, 380, 0, 20)
    StatusLabel.Position = UDim2.new(0.5, -190, 0.5, 70)
    StatusLabel.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    StatusLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    StatusLabel.Text = "Scanning for backdoors..."
    StatusLabel.TextSize = 18
    StatusLabel.Parent = StartFrame
    
    -- Main Frame UI Elements
    MainFrame.Size = UDim2.new(0, 450, 0, 200)
    MainFrame.Position = UDim2.new(0.5, -225, 0.5, -100)
    MainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    MainFrame.BorderSizePixel = 2
    MainFrame.Draggable = true
    MainFrame.Active = true
    MainFrame.BorderMode = Enum.BorderMode.Outline
    MainFrame.BorderColor3 = Color3.fromRGB(70, 70, 70)
    MainFrame.BackgroundTransparency = 0.1
    MainFrame.Visible = false
    
    -- Script TextBox
    ScriptBox.Size = UDim2.new(0, 380, 0, 40)
    ScriptBox.Position = UDim2.new(0.5, -190, 0.5, -30)
    ScriptBox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    ScriptBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    ScriptBox.PlaceholderText = "Place your require script here..."
    ScriptBox.TextSize = 18
    ScriptBox.Parent = MainFrame
    
    -- Helper function to create buttons
    local function createButton(name, text, position, callback, parent)
        local button = Instance.new("TextButton")
        button.Name = name
        button.Text = text
        button.Size = UDim2.new(0, 100, 0, 40)
        button.Position = position
        button.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        button.TextColor3 = Color3.fromRGB(255, 255, 255)
        button.Font = Enum.Font.SourceSans
        button.TextSize = 18
        button.BorderSizePixel = 2
        button.BorderColor3 = Color3.fromRGB(100, 100, 100)
        button.Parent = parent
        button.MouseButton1Click:Connect(callback)
        return button
    end
    
    -- Main UI Buttons
    ExecuteButton = createButton("ExecuteButton", "Execute", UDim2.new(0.5, 40, 0.5, 20), function()
        loadstring(ScriptBox.Text)()
    end, MainFrame)
    
    MinimizeButton = createButton("MinimizeButton", "-", UDim2.new(0.5, -140, 0.5, 20), function()
        MainFrame.Visible = false
        local Ball = Instance.new("Frame")
        Ball.Size = UDim2.new(0, 60, 0, 60)
        Ball.Position = UDim2.new(0.9, 0, 0.1, 0)
        Ball.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        Ball.Parent = ScreenGui
        Ball.Draggable = true
        Ball.Active = true
        Ball.MouseButton1Click:Connect(function()
            MainFrame.Visible = true
            Ball:Destroy()
        end)
    end, MainFrame)
    
    CloseButton = createButton("CloseButton", "X", UDim2.new(0.5, 40, 0.5, 20), function()
        ScreenGui:Destroy()
    end, MainFrame)
    
    -- Start UI Button
    ScanButton = createButton("ScanButton", "Start Scan", UDim2.new(0.5, -50, 0.5, 50), function()
        local backdoors = scanForBackdoors()
        
        -- Update progress bar and status
        for i = 1, 100 do
            Progress.Size = UDim2.new(i/100, 0, 1, 0)
            StatusLabel.Text = "Scanning... " .. i .. "%"
            wait(0.1)
        end
        
        -- Show main UI after scan
        StartFrame.Visible = false
        MainFrame.Visible = true
        
        -- Log found backdoors
        for _, v in pairs(backdoors) do
            print("Potential Backdoor:", v)
        end
        logBackdoors(backdoors)
    end, StartFrame)
    
    StartFrame.Parent = ScreenGui
    MainFrame.Parent = ScreenGui
    
    -- Smooth Animation for Buttons
    local function smoothButton(button)
        button.MouseEnter:Connect(function()
            local tween = TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(70, 70, 70)})
            tween:Play()
        end)
        button.MouseLeave:Connect(function()
            local tween = TweenService:Create(button, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(50, 50, 50)})
            tween:Play()
        end)
    end
    
    smoothButton(ScanButton)
    smoothButton(MinimizeButton)
    smoothButton(CloseButton)
    smoothButton(ExecuteButton)
end

-- Execute
antiCheatBypass()
createUI()

print("Backdoor Scanner - PRO Loaded.")

-- Extended Logging
local function logBackdoors(backdoors)
    local logFile = "backdoor_log.txt"
    local file = io.open(logFile, "a")
    for _, backdoor in pairs(backdoors) do
        file:write(backdoor .. "\n")
    end
    file:close()
end

-- Additional Features to extend length and functionality

-- Function to Display Logs in UI
local function displayLogs()
    local logFile = "backdoor_log.txt"
    local file = io.open(logFile, "r")
    if file then
        local logs = file:read("*all")
        file:close()
        
        local logFrame = Instance.new("Frame")
        logFrame.Size = UDim2.new(0, 450, 0, 300)
        logFrame.Position = UDim2.new(0.5, -225, 0.5, -150)
        logFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
        logFrame.BorderSizePixel = 2
        logFrame.Draggable = true
        logFrame.Active = true
        logFrame.BorderMode = Enum.BorderMode.Outline
        logFrame.BorderColor3 = Color3.fromRGB(70, 70, 70)
        logFrame.BackgroundTransparency = 0.1
        logFrame.Parent = game.CoreGui
        
        local logText = Instance.new("TextLabel")
        logText.Size = UDim2.new(1, -20, 1, -20)
        logText.Position = UDim2.new(0, 10, 0, 10)
        logText.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
        logText.TextColor3 = Color3.fromRGB(255, 255, 255)
        logText.Text = logs
        logText.TextSize = 14
        logText.TextWrapped = true
        logText.TextXAlignment = Enum.TextXAlignment.Left
        logText.TextYAlignment = Enum.TextYAlignment.Top
        logText.Parent = logFrame
        
        local closeLogButton = Instance.new("TextButton")
        closeLogButton.Size = UDim2.new(0, 100, 0, 40)
        closeLogButton.Position = UDim2.new(0.5, -50, 1, -50)
        closeLogButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        closeLogButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        closeLogButton.Text = "Close"
        closeLogButton.TextSize = 18
        closeLogButton.BorderSizePixel = 2
        closeLogButton.BorderColor3 = Color3.fromRGB(100, 100, 100)
        closeLogButton.Parent = logFrame
        closeLogButton.MouseButton1Click:Connect(function()
            logFrame:Destroy()
        end)
    end
end

-- Display Logs Button
local function createDisplayLogsButton(parent)
    local displayLogsButton = Instance.new("TextButton")
    displayLogsButton.Text = "Display Logs"
    displayLogsButton.Size = UDim2.new(0, 100, 0, 40)
    displayLogsButton.Position = UDim2.new(0.5, -50, 0.5, 60)
    displayLogsButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    displayLogsButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    displayLogsButton.TextSize = 18
    displayLogsButton.BorderSizePixel = 2
    displayLogsButton.BorderColor3 = Color3.fromRGB(100, 100, 100)
    displayLogsButton.Parent = parent
    displayLogsButton.MouseButton1Click:Connect(displayLogs)
    
    return displayLogsButton
end

-- Create Display Logs Button in Main Frame
createDisplayLogsButton(MainFrame)

-- Smooth Button Animation for Display Logs Button
smoothButton(displayLogsButton)